<div class="chat-window">
    <div class="chat-messages">
    {{#each messages}}
        {{this}}
    {{/each}}
    </div>
    <div class="chat-users"></div>
</div>
<form>
    <textarea name="text" rows="1" cols="40" tabindex="1"></textarea>
    <button type="submit" class="btn btn-success" tabindex="2"><i class="icon-ok icon-white"></i> Send</button>
</form>